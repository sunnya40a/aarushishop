
<!DOCTYPE html>
<html>
<head>
    <title>PostgreSQL Data</title>
    <!-- Include the Vue.js CDN -->
    <script src="https://cdn.jsdelivr.net/npm/vue@2.6.14"></script>

    <!-- Include a CSS framework (e.g., Bootstrap) for styling -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css">
</head>
<body>
    <div id="app">
        <table class="table table-striped">
            <thead>
                <tr>
                    <th>User ID</th>
                    <th>Username</th>
                    <th>Email</th>
                    <th>Password Hash</th>
                    <th>Comment</th>
                </tr>
            </thead>
            <tbody>
                <tr v-for="user in users" :key="user.UserID">
                    <td>{ user.UserID }</td>
                    <td> user.Username </td>
                    <td>$user.Email</td>
                    <td>{ PasswordHash }</td>
                    <td>.Comment</td>
                </tr>
            </tbody>
        </table>
    </div>

    <script>
        Vue.config.delimiters = ['${', '}']

        new Vue({
            el: "#app",
            data: {
                users: {users}
            }
        });
    </script>
</body>
</html>
